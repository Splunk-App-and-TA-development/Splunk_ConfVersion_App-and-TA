# ----------------------------------------------- #
# Configuration Monitoring TA for Splunk.         #
# ----------------------------------------------- #
# Splunk Add-on to monitor and parse Splunk       #
# configuration files. It allows administrators   #
# to quickly identify changes in Splunk's         #
# configuration.                                  #
# ----------------------------------------------- #
#     >> Default Permissions declarations <<      #
###################################################


# Application-level permissions
[]
access = read : [ admin, splunk-system-role ], write : [ admin, splunk-system-role ]
export = system

### VIEWS
[views]
access = read : [ admin, splunk-system-role ], write : [ admin, splunk-system-role ]
export = system

### EVENT TYPES
[eventtypes]
access = read : [ * ], write : [ admin, splunk-system-role ]
export = system

### PROPS
[props]
access = read : [ * ], write : [ admin, splunk-system-role ]
export = system

### TRANSFORMS
[transforms]
access = read : [ * ], write : [ admin, splunk-system-role ]
export = system

### LOOKUPS
[lookups]
access = read : [ * ], write : [ admin, splunk-system-role ]
export = system

### MACROS
[macros]
access = read : [ * ], write : [ admin, splunk-system-role ]
export = system

### COMMANDS
[commands]
access = read : [ * ], write : [ admin, splunk-system-role ]
export = system


### VIEWSTATES: even normal users should be able to create shared viewstates

[viewstates]
access = read : [ * ], write : [ * ]
export = system

